{{#each rows}}
<div class="row mb-5">
    <h3>{{this.nickname}}</h3>
</div>
<div class="row">
    <div class="col">
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">First Name</th>
                    <th scope="col">Last Name</th>
                    <th scope="col">Email</th>
                    <th scope="col">Phone</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <th scope="row">{{this.nickname}}</th>
                    <th scope="row">{{this.email}}</th>
                    <th scope="row">{{this.password}}</th>
                    <th scope="row">{{this.avatarPaths}}</th>
                </tr>
            </tbody>
        </table>
    </div>
    <div class="row">
        <div class="col">
            <b>Comments</b>
        </div>
    </div>
    <div class="row">
        <div class="col">
            
        </div>
    </div>
</div>
{{/each}}